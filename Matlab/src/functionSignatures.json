{
"_schemaVersion": "1.0.0",

"createWindow":
{
	"inputs":
	[
		{"name":"Fs", "kind":"required", "type":["numeric", "integer", "scalar"]},
		{"name":"length_t", "kind":"namevalue", "type":["numeric", "integer", "scalar"]},
		{"name":"overlap_percentage", "kind":"namevalue", "type":["numeric", "integer", "scalar"]},
		{"name":"window_type", "kind":"namevalue", "purpose":"Window function"}
	]
	
},

"spectralPitchDetector":
{
	"inputs":
	[
		{"name":"data", "kind":"required", "type":"numeric"},
		{"name":"window_struct", "kind":"required",  "purpose":"Window Structure generated by createWindow.m"},
		{"name":"Nfft", "kind":"required", "type":["numeric", "integer", "scalar"]},
		{"name":"f", "kind":"required", "type":"numeric", "purpose":"Frequency vector to map the detected pitch to an actual frequency value"}
	]
},

"pitchToKey":
{
	"inputs":
	[
		{"name":"pitch", "kind":"required", "type":["numeric", "scalar"], "purpose":"Pitch/Frequency to map to piano key"},
		{"name":"A4_tuning_val", "kind":"required", "type":["numeric", "scalar"], "purpose":"A4 key tuning value, will have an effect on the resulting key"}
	]
}


}
